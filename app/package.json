{
  "name": "aenco-smart-wallet",
  "version": "0.2.0",
  "description": "A multi blockchain wallet primarily supporting AENChain, Ethereum, ERC20 contracts, Bitcoin",
  "author": "Simon Ball <simon.ball@aencoin.com>",
  "private": true,
  "main": "index.js",
  "keywords": [
    "ecosystem:cordova"
  ],
  "scripts": {
    "bla": "cross-env BUILD_TARGET=android nuxt generate --no-lock",
    "clean": "rimraf www",
    "dev": "nuxt",
    "mobile-prepare": "npm run clean && cross-env BUILD_TARGET=android nuxt generate --no-lock && replace --recursive '/nuxt/' 'nuxt/' www",
    "cordova-build": "npm run mobile-prepare && cordova build",
    "build": "npm run clean && nuxt build --no-lock",
    "electron-dev": "cross-env NODE_ENV=DEV electron .",
    "electron-build": "nuxt build && electron-builder",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "test": "./node_modules/.bin/mocha --require @babel/register"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.4.1",
    "add": "^2.0.6",
    "bip32": "^2.0.2",
    "bip39": "^2.6.0",
    "bitcoinjs-lib": "^4.0.5",
    "chain-js-sdk": "^0.1.0",
    "chart.js": "^2.8.0",
    "cordova-android": "^8.0.0",
    "cordova-ios": "^5.0.1",
    "cross-env": "^5.2.0",
    "crypto-js": "^3.1.9-1",
    "date-fns": "^1.30.1",
    "ethereum-input-data-decoder": "^0.1.5",
    "ethereumjs-tx": "^1.3.7",
    "fs": "^0.0.2",
    "is-electron": "^2.2.0",
    "is-online": "^8.2.0",
    "lodash.debounce": "^4.0.8",
    "lodash.get": "^4.4.2",
    "nem2-library": "0.9.5",
    "nem2-sdk": "^0.10.1",
    "nuxt": "^2.6.3",
    "nuxt-matomo": "^1.2.2",
    "qrcode-generator": "^1.4.3",
    "rxjs": "^6.5.1",
    "vue-chartjs": "^3.4.2",
    "vue-clipboard2": "^0.3.0",
    "vue-i18n": "^8.10.0",
    "vue-password-strength-meter": "^1.4.2",
    "vue-recaptcha": "^1.1.1",
    "vue-tawk": "^1.0.1",
    "vue-youtube-embed": "^2.2.1",
    "vue2-dropzone": "^3.5.9",
    "vuebar": "^0.0.20",
    "vuejs-logger": "^1.5.3",
    "vuetify": "^1.5.13",
    "vuetify-loader": "^1.2.2",
    "vuetify-upload-button": "^1.2.2",
    "vuex-persistedstate": "^2.5.4",
    "web3": "^1.0.0-beta.52",
    "yarn": "^1.22.0",
    "zxcvbn": "^4.4.2"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.4.3",
    "@babel/register": "^7.4.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.17",
    "@fortawesome/free-solid-svg-icons": "^5.8.1",
    "@fortawesome/vue-fontawesome": "^0.1.6",
    "@nuxtjs/eslint-config": "^0.0.1",
    "@vue/test-utils": "^1.0.0-beta.27",
    "babel-eslint": "^10.0.1",
    "babel-plugin-transform-es2015-spread": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-stage-0": "^6.24.1",
    "browser-env": "^3.2.6",
    "chai": "^4.2.0",
    "cordova": "^9.0.0",
    "cordova-plugin-whitelist": "^1.3.3",
    "createjs": "^1.0.1",
    "electron": "^4.1.5",
    "electron-builder": "^20.39.0",
    "electron-devtools-installer": "^2.2.4",
    "eslint": "^5.16.0",
    "eslint-config-standard": ">=12.0.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jest": "^22.5.1",
    "eslint-plugin-node": ">=8.0.1",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": ">=4.0.0",
    "eslint-plugin-vue": "^5.2.2",
    "mocha": "^6.1.3",
    "nodemon": "^1.18.11",
    "replace": "^1.1.0",
    "require-extension-hooks": "^0.3.3",
    "require-extension-hooks-babel": "^0.1.1",
    "require-extension-hooks-vue": "^2.0.0",
    "rimraf": "^2.6.3",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "vue-template-compiler": "^2.6.10"
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-whitelist": {}
    },
    "platforms": [
      "android",
      "ios"
    ]
  }
}